<body>
    <!-- Breadcrumb Section Begin -->
    <section class="breadcrumb-section set-bg" data-setbg="/img/hero/hinh-1.jpg">
        <div class="container mt-5">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="breadcrumb-text">

                        <div class="bt-option">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Breadcrumb Section End -->

    <div class="main-wrapper" style="padding-top: 72.8px;">
        <nav class="breadcrumb-text wow fadeInDown" style="visibility: visible; animation-name: fadeInDown;">
            <div class="container">
                <div class="breadcrumb-title">@Model.Name</div>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a asp-area="" asp-controller="Gyms" asp-action="HeThongGym">Câu lạc bộ</a></li>
                    <li class="breadcrumb-item active">@Model.Name</li>
                </ol>
            </div>
        </nav>
        <a href="">
            <div class="main-breadcrumb" style="opacity: 1;">
                <img src="~/Hinh/HinhGym/@Model.Images" alt="@Model.Name" style="width: 100%; height: auto; border-radius: 5px; margin-bottom: 10px;" />
            </div>
    </div>
    </a>
    <section class="section section-club-detail-info">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 content-text pr-lg-5 m-b-md-5 wow fadeInLeft" style="visibility: visible; animation-name: fadeInLeft;">
                    <div class="feature-title" style="background-image: url(/Hinh/HinhGym/sosanhgoi.jpg);">@Model.Name</div>
                    <div class="sidebar-body">
                        <div class="item-border">
                            <div class="m-b-24px">
                                <div class="m-b-24px">
                                    <h1 class="h6 title-s1">CLB @Model.Name - Khu vườn fitness trong thành phố</h1>
                                </div>
                                <div class="h6 title-s1">Địa điểm</div>
                                <div class="icon-list">
